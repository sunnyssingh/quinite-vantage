# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Enable Real-time AI
ENABLE_REAL_CALLING=true
ENABLE_REALTIME_AI=true

# OpenAI
OPENAI_API_KEY=sk-proj-your_openai_api_key_here

# Site URL (for WebSocket)
NEXT_PUBLIC_SITE_URL=https://your-domain.com

# WebSocket Server URL
WS_URL=wss://your-websocket-server.com

# Plivo
PLIVO_AUTH_ID=your_plivo_auth_id_here
PLIVO_AUTH_TOKEN=your_plivo_auth_token_here
PLIVO_PHONE_NUMBER=+1234567890

# Razorpay (for subscription payments)
RAZORPAY_KEY_ID=your_razorpay_key_id_here
RAZORPAY_KEY_SECRET=your_razorpay_key_secret_here
NEXT_PUBLIC_RAZORPAY_KEY_ID=your_razorpay_key_id_here
RAZORPAY_WEBHOOK_SECRET=your_razorpay_webhook_secret_here
